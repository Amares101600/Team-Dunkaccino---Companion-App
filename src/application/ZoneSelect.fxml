<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controller.ZoneSelectController">
   <children>
      <ImageView fitHeight="720.0" fitWidth="1280.0" pickOnBounds="true">
         <image>
           <Image url="pics/Hades_Underworld_Map.jpg" />
         </image>
      </ImageView>
      <Button fx:id="HoHButton" layoutX="120.0" layoutY="109.0" mnemonicParsing="false" onAction="#HoHPressed" prefHeight="150.0" prefWidth="400.0" text="House of Hades" textFill="#ef2525">
         <font>
            <Font name="Bell MT Bold" size="39.0" />
         </font>
      </Button>
      <Button fx:id="BackButton" layoutX="30.0" layoutY="30.0" mnemonicParsing="false" onAction="#BackPressed" prefHeight="50.0" prefWidth="90.0" text="BACK" />
      <Button fx:id="ElysiumButton" layoutX="763.0" layoutY="201.0" mnemonicParsing="false" onAction="#ElysiumPressed" prefHeight="150.0" prefWidth="400.0" text="Elysium" textFill="#ef2525">
         <font>
            <Font name="Bell MT Bold" size="40.0" />
         </font>
      </Button>
      <Button fx:id="TartarusButton" layoutX="120.0" layoutY="319.0" mnemonicParsing="false" onAction="#TartarusPressed" prefHeight="150.0" prefWidth="400.0" text="Tartarus" textFill="#ef2525">
         <font>
            <Font name="Bell MT Bold" size="40.0" />
         </font>
      </Button>
      <Button fx:id="AsphodelButton" layoutX="120.0" layoutY="523.0" mnemonicParsing="false" onAction="#AsphodelPressed" prefHeight="150.0" prefWidth="400.0" text="Asphodel" textFill="#ef2525">
         <font>
            <Font name="Bell MT Bold" size="40.0" />
         </font>
      </Button>
      <Button fx:id="GreeceButton" layoutX="763.0" layoutY="416.0" mnemonicParsing="false" onAction="#GreecePressed" prefHeight="150.0" prefWidth="400.0" text="Greece" textFill="#ef2525">
         <font>
            <Font name="Bell MT Bold" size="40.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
